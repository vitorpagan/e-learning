<div class="row">
  <div class="col-md-offset-2 col-md-8">
    <h1><%= @public_target.name %></h1>
    <div class="form-group">
      <label>Data de Criação:</label> <%= l(@public_target.created_at, format: :full) %>
    </div>
    <div class="text-center">
      <%= link_to 'Voltar', admin_public_targets_path, class: 'btn btn-default' %> <%= link_to 'Editar', edit_admin_public_target_path(@public_target), class: 'btn btn-primary' %>
    </div>
  </div>
</div>